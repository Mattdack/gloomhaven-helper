{{!-- add party to campaign --}}
  <script src="https://code.jquery.com/jquery-3.6.0.js"></script>
  <script src="https://code.jquery.com/ui/1.13.2/jquery-ui.js"></script>
<main class="flex grid grid-cols-1 p-8 justify-items-center md:grid-cols-2 xl:grid-cols-3">

    <div class="m-2 p-2">
        <h2>Who is in your party?</h2>
        {{>addParty}}
    </div>
    </div>

<div class="ui-widget">
  {{!-- <label for="chooseyourmonster">Monsters: </label>
  <input id="{chooseyourmonster}">
  <label>lvl:</label>
  <input type="number" id="chooseyourmonsterlvl"/> --}}
 
  <ul> {{#each monsters}}
    <li data-mid="{{id}}">{{name}},lvl:{{level}}<button class="chooseyourmonster">+</button></li>
  {{/each}}
  </ul>
  
  
</div>

    <div class="flex-row">
        <div class="m-3 p-3">
            {{>newmonster}}
        </div>
    </div>

    <div class="flex-row">
        <div class="m-2 p-2">
            {{>monstersaddedlist}}
        </div>
    </div>
    <div class="flex-row">
        <div class="m-2 p-2">
            <button>Do the thing!</button>
        </div>
    </div>
</main>
<script src="/js/addMonsters.js"></script>
<script src="/js/encounter.js"></script>
<script src="/js/addParty.js"></script>